        <DTS:Executable
          DTS:refId="Package\Load Configuration Tables\Check Incoming KPI Periods Data"
          DTS:CreationName="Microsoft.ExecuteSQLTask"
          DTS:Description="Execute SQL Task"
          DTS:DTSID="{1246D0D3-49B5-4267-BB03-D8E4BDFB3E9E}"
          DTS:ExecutableType="Microsoft.ExecuteSQLTask"
          DTS:LocaleID="-1"
          DTS:ObjectName="Check Incoming KPI Periods Data"
          DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2017 RC1; © 2017 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
          DTS:ThreadHint="0">
          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{5C37F834-DC43-4471-AE9C-FE2FEA2F3BB6}"
              SQLTask:SqlStatementSource="----------------------------------------------------------------------------------------------------------------------------------------------------&#xA;/*&#xA;Seeing as this is a very sensitive file that will be wildsly used, it is crucial to make sure that there are no problems with the incoming data.&#xA;If there is overlap between two ranges not from the first year follow-up meetings, the rows will not be loaded to the table.&#xA;The problematic rows will be recorded in a separate table for debugging purposes.&#xA;&#xA;If the check below returns 1 then the data is okay and will be loaded.&#xA;*/&#xA;----------------------------------------------------------------------------------------------------------------------------------------------------&#xA;; &#xA;WITH CheckPeriods AS (&#xA;&#x9;SELECT &#xA;&#x9;&#x9; A.[Order]&#xA;&#x9;&#x9;,A.Base_Period_Name&#xA;&#x9;&#x9;,B.Base_Period_Name as Next_Base_Period_Name&#xA;&#x9;&#x9;,A.Insurer&#xA;&#x9;&#x9;,A.Range_Max_Days&#xA;&#x9;&#x9;,B.Range_Min_Days as Next_Range_Min_Days&#xA;&#x9;&#x9;,CASE WHEN A.Insurer = B.Insurer AND DATEDIFF(DAY,A.Range_Max_Days,B.Range_Min_Days) &lt;&gt; 1 THEN 1 ELSE 0 END as Is_Problem&#xA;&#x9;FROM ext.KPI_Period_Definitions A&#xA;&#x9;LEFT JOIN ext.KPI_Period_Definitions B ON A.[Order] = B.[Order] - 1&#xA;&#x9;WHERE A.[Order] &gt; 1 -- exclude &quot;all periods&quot; and &quot;IC&quot;&#xA;&#x9;AND A.Period_Name NOT LIKE '%FU Meeting'&#xA;&#x9;AND B.Period_Name NOT LIKE '%FU Meeting' &#xA;)&#xA;SELECT &#xA;&#x9; [Order]&#xA;&#x9;,Base_Period_Name&#xA;&#x9;,Next_Base_Period_Name&#xA;&#x9;,Insurer&#xA;&#x9;,Range_Max_Days&#xA;&#x9;,Next_Range_Min_Days&#xA;&#x9;,GETDATE() as DB_Create_Date&#xA;INTO #Temp&#xA;FROM CheckPeriods&#xA;WHERE Is_Problem = 1&#xA;&#xA;INSERT INTO adm.KPI_Period_Definitions_Problems&#xA;SELECT * FROM #Temp&#xA;&#xA;DECLARE @Num as int&#xA; &#xA;SET @Num = (SELECT COUNT(*) &#xA;&#x9;&#x9;&#x9;FROM #Temp)&#xA;BEGIN &#xA;&#x9;IF (@Num &gt; 0 )&#xA;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;SELECT 0 as Result-- error&#xA;&#x9;&#x9;END&#xA;&#x9;&#xA;&#x9;SELECT 1 as Result -- okay to load&#xA;END&#xA;"
              SQLTask:ResultType="ResultSetType_SingleRow" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
              <SQLTask:ResultBinding
                SQLTask:ResultName="Result"
                SQLTask:DtsVariableName="User::KPI_Result" />
            </SQLTask:SqlTaskData>
          </DTS:ObjectData>
        </DTS:Executable>
